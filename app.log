    <!-- AOS Library for Animations -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/aos/2.3.4/aos.css" rel="stylesheet">

    <style>
        :root {
            --primary-color: #020B1C;
            --secondary-color: #0A1731;
            --accent-color: #3B82F6;
            --text-primary: #FFFFFF;
            --text-secondary: #94A3B8;
        }

        /* General Styles */
        body {
            background-color: var(--primary-color);
            color: var(--text-primary);
            font-family: 'Inter', sans-serif;
            line-height: 1.6;
        }

        .text-gradient {
            background: linear-gradient(45deg, #3B82F6, #9333EA);
            background-clip: text;
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
        }

        /* Navbar Styles */
        .navbar {
            background-color: rgba(2, 11, 28, 0.95);
            backdrop-filter: blur(10px);
            padding: 1rem 0;
        }

        .navbar-brand {
            color: var(--accent-color) !important;
            font-weight: bold;
            font-size: 1.5rem;
        }

        /* Hero Section */
        .hero-section {
            min-height: 100vh;
            position: relative;
            overflow: hidden;
            padding: 100px 0;
        }

        .hero-bg {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background:
                radial-gradient(circle at 20% 30%, rgba(147, 51, 234, 0.15) 0%, transparent 30%),
                radial-gradient(circle at 80% 70%, rgba(59, 130, 246, 0.15) 0%, transparent 30%);
        }

        .hero-img-container {
            position: relative;
            width: 400px;
            height: 400px;
            margin: 0 auto;
        }

        .hero-img-container::before,
        .hero-img-container::after {
            content: '';
            position: absolute;
            width: 200px;
            height: 200px;
            border-radius: 50%;
            filter: blur(40px);
        }

        .hero-img-container::before {
            top: -20px;
            right: -20px;
            background: rgba(147, 51, 234, 0.3);
        }

        .hero-img-container::after {
            bottom: -20px;
            left: -20px;
            background: rgba(59, 130, 246, 0.3);
        }

        .hero-img {
            position: relative;
            width: 100%;
            height: 100%;
            object-fit: cover;
            border-radius: 20px;
            border: 4px solid rgba(59, 130, 246, 0.2);
            z-index: 1;
        }

        /* About Section */
        .terminal-window {
            background-color: var(--secondary-color);
            border-radius: 10px;
            overflow: hidden;
            border: 1px solid rgba(59, 130, 246, 0.2);
            box-shadow: 0 4px 20px rgba(0, 0, 0, 0.3);
        }

        .terminal-header {
            background-color: rgba(0, 0, 0, 0.3);
            padding: 12px 20px;
            display: flex;
            align-items: center;
            gap: 8px;
        }

        .terminal-button {
            width: 12px;
            height: 12px;
            border-radius: 50%;
        }

        .button-red { background-color: #ff5f56; }
        .button-yellow { background-color: #ffbd2e; }
        .button-green { background-color: #27c93f; }

        .terminal-body {
            padding: 24px;
            font-family: 'Fira Code', monospace;
            line-height: 1.6;
        }

        /* Skills Section */
        .skill-card {
            background-color: var(--secondary-color);
            border: 1px solid rgba(59, 130, 246, 0.1);
            border-radius: 10px;
            padding: 24px;
            height: 100%;
            transition: transform 0.3s ease;
        }

        .skill-card:hover {
            transform: translateY(-5px);
        }

        .skill-icon {
            font-size: 2rem;
            color: var(--accent-color);
            margin-bottom: 1rem;
        }

        /* Projects Section */
        .project-card {
            background-color: var(--secondary-color);
            border: 1px solid rgba(59, 130, 246, 0.1);
            border-radius: 10px;
            overflow: hidden;
            transition: all 0.3s ease;
            height: 100%;
        }

        .project-card:hover {
            transform: translateY(-8px);
            box-shadow: 0 8px 24px rgba(59, 130, 246, 0.1);
        }

        .project-img {
            height: 240px;
            object-fit: cover;
            width: 100%;
        }

        .skills-container {
            margin-bottom: 1.5rem;
        }

        .skills-group {
            margin-bottom: 1rem;
        }

        .skills-group h6 {
            color: var(--accent-color);
            font-size: 0.9rem;
            font-weight: 600;
            margin-bottom: 0.5rem;
        }

        .project-tag {
            background-color: rgba(59, 130, 246, 0.1);
            color: var(--accent-color);
            padding: 6px 12px;
            border-radius: 6px;
            font-size: 0.85rem;
            transition: all 0.2s ease;
            display: inline-block;
            margin: 0.25rem;
        }

        .project-tag:hover {
            background-color: rgba(59, 130, 246, 0.2);
        }

        /* Certificates Section */
        .cert-card {
            background-color: var(--secondary-color);
            border: 1px solid rgba(59, 130, 246, 0.1);
            border-radius: 10px;
            overflow: hidden;
            transition: all 0.3s ease;
            height: 100%;
        }

        .cert-card:hover {
            transform: translateY(-8px);
            box-shadow: 0 8px 24px rgba(59, 130, 246, 0.1);
        }

        .cert-img {
            height: 180px;
            object-fit: cover;
            width: 100%;
        }

        /* Contact Section */
        .contact-section {
            background-color: var(--secondary-color);
            border-radius: 10px;
            padding: 2rem;
        }

        .social-icon {
            color: var(--accent-color);
            font-size: 1.5rem;
            transition: all 0.3s ease;
            text-decoration: none;
        }

        .social-icon:hover {
            color: var(--text-primary);
            transform: translateY(-3px);
        }

        /* Buttons */
        .btn-primary {
            background-color: var(--accent-color);
            border: none;
            padding: 12px 24px;
            border-radius: 8px;
            transition: all 0.3s ease;
        }

        .btn-primary:hover {
            background-color: #2563eb;
            transform: translateY(-2px);
        }

        .btn-outline {
            background-color: transparent;
            border: 1px solid var(--accent-color);
            color: var(--accent-color);
            padding: 12px 24px;
            border-radius: 8px;
            transition: all 0.3s ease;
            text-decoration: none;
        }

        .btn-outline:hover {
            background-color: rgba(59, 130, 246, 0.1);
            transform: translateY(-2px);
        }

        /* Footer */
        footer {
            background-color: var(--secondary-color);
            padding: 2rem 0;
            margin-top: 4rem;
        }
    </style>
</head>

